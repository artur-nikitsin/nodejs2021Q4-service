{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon src/server.ts --legacy-watch --exitcrash",
    "nest:start": "nest start",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "build": "tsc -p tsconfig.json"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "resolutions": {
    "formidable": "2.0.1"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjsi/class-validator": "^0.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "fastify": "^3.24.1",
    "fastify-auth": "^1.1.0",
    "fastify-bcrypt": "^1.0.0",
    "fastify-typeorm": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "^0.2.41",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^5.12.1",
    "typescript": "^3.9.10"
  }
}
